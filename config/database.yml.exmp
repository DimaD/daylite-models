#database connection info.
#Don't touch this file if you use it as rails plugin
#Specify these daylite: settings in your rails database.yml instead

daylite:
    adapter: postgresql
    encoding: unicode
    sslmode: require
    host: localhost
    port: 6181
    database: MyOrganization
    username: my_admin_username
    password: my_admin_password
    pool: 5
    reaping_frequency: 10

